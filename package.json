{
  "name": "beCamp",
  "version": "1.0.0",
  "description": "Open source website for be.camp. ",
  "author": "Andrew Boyd <andrew@wearebraid.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=dev HOST=0.0.0.0 nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
