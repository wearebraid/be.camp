    "@nuxtjs/style-resources": "^0.1.2",
    "airtable": "^0.7.1",
    "blueimp-md5": "^2.10.0",
    "buttercms": "^1.1.5",
    "clone-deep": "^4.0.0",
    "moment": "^2.29.2",
    "node-sass": "^4.9.3",
    "nuxt": "^2.5.1",
    "reset-css": "^4.0.1",
    "sass-loader": "^7.1.0",
    "vue-style-loader": "^4.1.2",
    "vuex-persistedstate": "^2.5.4",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.6.0",
